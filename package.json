{
  "name": "kucc2023.cc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www.js",
    "start:dev": "set NODE_ENV=production && node ./dist/bin/www.js",
    "tsc": "del-cli --force ./dist && tsc --project tsconfig.client.build.json && tsc --project tsconfig.server.json",
    "copy": "cp -r client/js/plugins dist/client/js && cp -r client/stylesheets dist/client && cp -r views dist/ && cp .env dist/",
    "tsc:watch": "tsc -w -p tsconfig.client.dev.json",
    "dev": "set NODE_ENV=development && nodemon bin/www.ts",
    "build": "pnpm run tsc && pnpm run copy"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "2.6.9",
    "ejs": "2.6.2",
    "express": "4.16.4",
    "http-errors": "1.6.3",
    "morgan": "1.9.1",
    "vue": "3.2.11"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.4",
    "@types/debug": "4.1.9",
    "@types/express": "4.17.19",
    "@types/morgan": "1.9.6",
    "@types/node": "20.8.4",
    "del-cli": "5.1.0",
    "dotenv": "16.3.1",
    "nodemon": "3.0.1",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  }
}
